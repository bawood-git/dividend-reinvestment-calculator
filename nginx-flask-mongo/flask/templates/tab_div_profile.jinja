<div class="divContent" style="width: 800px;">
    <span class="tabTitle">Company Profile</span>
    <div class="divInnerContent" style="height: 300px;">
        <form action="{{ url_for('report') }}" method="post" name="profileForm">
            <input type="hidden" name="csrf_token" value = "{{ csrf_token() }}" />
            <table width="100%">
                <tr>
                    <tr>
                        <th>Ticker</th>
                        <td>{{quote['Global Quote']['01. symbol']}}</td>
                    </tr>
                    <tr>
                        <th>Name</th>
                        <td>{{overview['Name']}}</td>
                    </tr>
                    <tr>
                        <th>Description</th>
                        <td>{{overview['Description']}}</td>
                    </tr>
                    <tr>
                        <th>Site</th>
                        <td>{{overview['OfficialSite']}}</td>
                    </tr>
                    <tr>
                        <th>Sector</th>
                        <td>{{overview['Sector']}}</td>
                    </tr>
                    <tr>
                        <th>Industry</th>
                        <td>{{overview['Industry']}}</td>
                    </tr>
                    <tr>
                        <th>Country</th>
                        <td>{{overview['Country']}}</td>
                    </tr>
                    <tr>
                        <th>Exchange</th>
                        <td>{{overview['Exchange']}}</td>
                    </tr>
                </tr>
            </table>
        </form>
    </div>
</div>