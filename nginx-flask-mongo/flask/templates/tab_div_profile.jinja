<div class="divContent" style="width: 800px;">
    <table width="100%">
        <tr>
            <td><span class="tabTitle">Company Profile</span></td>
            <td style="text-align: right;"><a href="{{url_for('news',symbol=model.profile['stock_symbol'])}}" target="_blank">News</a></td>
        </tr>
    </table>
    <div class="divInnerContent" style="height: 300px;">
            <table width="100%">
                <tr>
                    <tr>
                        <th>Sentiment</th>
                        <td>{{model.sentiment}}</td>
                    </tr>
                    <tr>
                        <th>Ticker</th>
                        <td>{{model.profile['stock_symbol']}}</td>
                    </tr>
                    <tr>
                        <th>Name</th>
                        <td>{{model.profile['company_name']}}</td>
                    </tr>
                    <tr>
                        <th>Description</th>
                        <td>{{model.profile['company_desc']}}</td>
                    </tr>
                    <tr>
                        <th>Site</th>
                        <td>{{model.profile['company_website']}}</td>
                    </tr>
                    <tr>
                        <th>Sector</th>
                        <td>{{model.profile['stock_sector']}}</td>
                    </tr>
                    <tr>
                        <th>Industry</th>
                        <td>{{model.profile['stock_industry']}}</td>
                    </tr>
                    <tr>
                        <th>Exchange</th>
                        <td>{{model.profile['exchange']}}</td>
                    </tr>
                </tr>
            </table>
    </div>
</div>