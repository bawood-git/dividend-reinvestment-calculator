<div class="divContent">
    <table width="100%">
        <tr>
            <td><span class="tabTitle">Report Summary</span></td>
            <td style="text-align: right;"><a href="" target="_blank"></a></td>
        </tr>
    </table>
    <div class="divInnerContent" style="height:366px">
        <table>
            <tr>
                <th style="border-bottom: 1px solid black;" colspan="2">Vestment</th>
            </tr>
            <tr>
                <th>Years Vested</th>
                <td style="text-align: left;">{{ "{:.2f}".format(totals['years_vested'])}}</td>
            </tr>
            <tr>
                <th>Total Contributions</th>
                <td style="text-align: left;">${{ "{:,.2f}".format(totals['contributions'])}}</td>
            </tr>
            <tr>
                <th style="padding-top:3%; border-bottom: 1px solid black;">Assets</th>
                <th style="padding-top:3%; border-bottom: 1px solid black;" >{{"${:,.2f}".format(totals['ending_assets'] + totals['cash'])}}</td>

            </tr>
            <tr>
                <th>Cash</th>
                <td style="text-align: left;">{{"${:,.2f}".format(totals['cash'])}}</td>
            </tr>
            <tr>
                <th>Securities</th>
                <td style="text-align: left;">{{"${:,.2f}".format(totals['ending_assets'])}}</td>
            </tr>
            <tr>
                <th>Shares Owned</th>
                <td style="text-align: left;">{{"{:,.2f}".format(totals['shares_owned'])}}</td>
            </tr>
            <tr>
                <th style="padding-top:3%; border-bottom: 1px solid black;">Dividends</th>
                <th style="padding-top:3%; border-bottom: 1px solid black;">${{"{:,.2f}".format(totals['ending_distribution'])}}</th>
            </tr>
            <tr>
                <th>Avg Periodic Growth</th>
                <td style="text-align: left;">
                ${{"{:,.2f}".format(totals['distribution_growth_avg'])}}
                </td>
            </tr>
            <tr>
                <th>Total Growth</th>
                <td style="text-align: left;">
                    ${{"{:,.2f}".format(totals['distribution_growth_tot'])}} / {{"{:,.2f}".format(totals['distribution_growth_pct'])}}%
                </td>
            </tr>
            <tr>
                <th>Annual Total</th>
                <td style="text-align: left;">{{"${:,.2f}".format(totals['income_annual'])}}</td>
            </tr>
            <tr>
                <th>Total Reinvested</th>
                <td style="text-align: left;">{{"${:,.2f}".format(totals['total_reinvested'])}}</td>
            </tr>
        </table>
    </div>
</div>
