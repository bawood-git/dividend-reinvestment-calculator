{% from 'bootstrap5/form.html' import render_form %}
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <link rel="stylesheet" href="{{ url_for('static', filename='style.css') }}">
        <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    </head>
    <title>DivCalc Search</title>
    <body>
        <div class="page">
            {{header|safe}}
            <table width="100%">
                <tr style="padding-top: 15px;">
                    <td style="width: 600px; padding-right: 15px; padding-top: 15px;">{{profile|safe}}</td>
                    <td style="padding-top: 15px;">{{finance|safe}}</td>
                </tr>
                <tr>
                    <td style="width: 600px; padding-right: 15px; padding-top: 15px;">
                        {{chart|safe}}
                    </td>
                    <td style="padding-top: 15px;">{{history|safe}}</td>
                </tr>
                <tr>
                    <td colspan="2" style="padding-top: 15px;">{{settings|safe}}</td>
                </tr>
            </table>
        </div>
    </body>
</html>